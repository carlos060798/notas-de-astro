---
import Card from "./card.astro";
const  response= await fetch('https://jsonplaceholder.typicode.com/posts')
const data=  await response.json();

console.log(data);

---
<div>
  {
    data.map((item)=>{
      return(
        <Card  id={item.id}title={item.title} body={item.body} />
      )
    })
  }

</div>